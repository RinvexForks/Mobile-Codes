{
    "name": "faustbrian/mobile-codes",
    "description": "Up-To-Date list of MCC/MNC Codes from mcc-mnc.com in multiple formats",
    "keywords": ["mobile-codes", "Mobile Codes", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "fabpot/goutte": "^3.1",
        "faustbrian/payload": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\MobileCodes\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
